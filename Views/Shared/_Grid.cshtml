@using System.Data
@model DataTable

@{
    Layout = null;
}

<table class="datatable table table-hover non-hover table-striped" style="width:100%">
    <tr>
        @foreach (DataColumn col in Model.Columns)
        {
            <th>@col.ColumnName</th>
        }

    </tr>
    @foreach (DataRow row in Model.Rows)
    {
        <tr class="gra">
            @foreach (DataColumn col in Model.Columns)
            {
                <td>@row[col.ColumnName]</td>
            }
        </tr>
    }
</table>

@Html.Partial("_LayoutReport");
